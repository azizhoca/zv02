  <property name="styleSheet">
   <string notr="true">/* Global Styles */
* {
	font: 10pt &quot;Segoe UI&quot;, Tahoma, Geneva, Verdana, sans-serif;;
    color: #333; /* Koyu gri yazı rengi */
    margin: 0;
    padding: 0;
    font-weight: bold;
}

/* Etiketler */
QLabel {
    color: #333; /* Koyu gri yazı rengi */
    font-weight: 500; /* Orta kalınlıkta yazı */
}

/* Butonlar */
QPushButton {
    color: #fff; /* Beyaz yazı rengi */
    background-color: #007bff; /* Modern mavi arka plan */
    border: none;
    border-radius: 6px; /* Hafif yuvarlatılmış köşeler */
    padding: 8px 20px; /* Rahat içerik alanı */
    margin: 5px;
    font-weight: 600; /* Kalın yazı */
    text-align: center;
}

QPushButton:hover {
    background-color: #0056b3; /* Üzerine gelindiğinde koyu mavi */
}

QPushButton:pressed {
    background-color: #004080; /* Tıklama anında daha koyu mavi */
}

/* Araç Butonları */
QToolButton {
    color: #333;
    background-color: transparent;
    border: none;
    border-radius: 4px;
    font-weight: 600;
    text-align: center;
    font-size: 12px; /* Yazı boyutu ayarlandı */
}

QToolButton:hover {
    background-color: #e0e0e0; /* Üzerine gelindiğinde hafif gri */
}

QToolButton:pressed {
    background-color: #d0d0d0; /* Tıklama anında daha koyu gri */
}

/* Giriş Alanları */
QLineEdit, QPlainTextEdit {
    color: #333;
    background-color: #fff; /* Beyaz arka plan */
    border: 1px solid #ccc; /* Açık gri sınır */
    border-radius: 4px; /* Hafif yuvarlatılmış köşeler */
    font-size: 14px;
    padding: 6px;
}

QLineEdit:focus, QPlainTextEdit:focus {
    border-color: #007bff; /* Odaklanıldığında mavi sınır */
}

/* Seçim Kutuları */
QComboBox {
    color: #333;
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 4px;
    padding: 5px;
}

QComboBox::drop-down {
    border-left: 1px solid #ccc; /* Düğme ile kutuyu ayır */
}

/* Menüler ve Menü Çubuğu */
QMenuBar {
    background-color: #007bff;
    color: #fff;
    padding: 0 10px;
}

QMenuBar::item {
    padding: 10px;
    background-color: transparent;
}

QMenuBar::item:selected {
    background-color: #0056b3;
}

QMenu {
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 4px;
    padding: 5px;
}

QMenu::item:selected {
    background-color: #007bff;
    color: #fff;
}

QMenu::separator {
    height: 1px;
    background-color: #ccc;
    margin: 5px 0;
}

/* Checkbox ve Radio Butonlar */
QCheckBox, QRadioButton {
    color: #333;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #ccc;
    border-radius: 8px;
    background-color: #fff;
}

QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background-color: #007bff;
    border: 1px solid #007bff;
}

/* Tablolar */
QTableWidget {
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 4px;
    font-size: 14px;
}

QTableWidget::item:selected {
    background-color: #007bff;
    color: #fff;
}

QHeaderView::section {
    background-color: #f5f5f5;
    border: 1px solid #ccc;
    padding: 8px;
    font-weight: 600;
    font-size: 14px;
}

/* Kaydırıcılar */
QScrollBar:vertical, QScrollBar:horizontal {
    background-color: #f5f5f5;
    border: none;
    width: 12px;
    margin: 0;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #ccc;
    border-radius: 6px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
    width: 0px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* GroupBox */
QGroupBox {
    border: 1px solid #ccc;
    border-radius: 8px;
    margin: 10px;
    padding: 10px;
    background-color: #fff;
}

QGroupBox::title {
    subcontrol-origin: padding;
    subcontrol-position: top left;
    padding: 0 10px;
    color: #333;
    font-weight: 600;
    font-size: 16px;
}
</string>
